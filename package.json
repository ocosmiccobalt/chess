{
  "name": "chess",
  "version": "1.0.0",
  "private": true,
  "description": "Landing page for a fictional chess tournament",
  "type": "module",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/css/**/*.css\"",
    "eslint": "eslint source/js/",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}",
    "source/css/**/*.css"
  ],
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@htmlacademy/editorconfig-cli": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "browser-sync": "^3.0.2",
    "del": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-htmlacademy": "^10.0.1",
    "gh-pages": "^6.1.1",
    "gulp": "^5.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^10.0.0",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-svgstore": "^9.0.0",
    "postcss-csso": "^6.0.1",
    "postcss-custom-properties": "^13.3.12",
    "postcss-import": "^16.1.0",
    "posthtml-include": "^2.0.1",
    "stylelint": "^16.6.1",
    "stylelint-config-htmlacademy": "^4.2.3",
    "webpack": "^5.92.1",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "core-js": "^3.37.1"
  }
}
